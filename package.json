{
	"name": "pixelmind-monorepo",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"start": "turbo run start",
		"start:sample-agent-cli": "turbo start --filter @PixelMind/sample-agentkit-cli",
		"dev:frontend-example": "turbo dev --filter @PixelMind/frontend-example",
		"build:frontend-example": "turbo start --filter @PixelMind/frontend-example",
		"lint:frontend-example": "turbo start --filter @PixelMind/frontend-example",
		"preview:frontend-example": "turbo start --filter @PixelMind/frontend-example"
	},
	"devDependencies": {
		"turbo": "^2.4.0",
		"vite": "6.0.11"
	},
	"packageManager": "npm@11.1.0"
}
